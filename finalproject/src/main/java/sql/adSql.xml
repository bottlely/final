<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="mars.ad">
  
  	<select id="adList" parameterType="Map" resultType="mars.ad.model.ApplyAdDTO">
  		select * from ad where status=0
  	</select>
  	
  	<insert id="addAdForm" parameterType="mars.ad.model.ApplyAdDTO">
  	insert into ad values(ad_ad_idx.nextval, 32, #{name}, #{type}, #{link}, 'no', #{fee}, #{favor_movie}, #{favor_sport}, #{favor_fashion}, #{favor_beauty}, #{favor_travel}, #{favor_music}, #{favor_dance}, #{favor_food}, 0, '', '', sysdate, '', '', '', '', '', '', '', '', #{s_date}, #{e_date})
  	</insert>
  </mapper>