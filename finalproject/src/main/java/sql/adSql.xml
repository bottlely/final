<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="mars.ad">
  
  	<select id="adList" parameterType="Map" resultType="mars.ad.model.ApplyAdDTO">
  		select * from ad where status=0
  	</select>
  	
  	<insert id="addAdForm" parameterType="mars.ad.model.ApplyAdDTO">
  		insert into ad values(ad_ad_idx.nextval, #{member_idx}, #{name}, #{type}, #{link}, #{ad_content}, '05-11-17', '31-12-17', 2000000, 0, 1, 1, 0, 1, 0, 1, 0, 0, #{post}, 'no_one', sysdate, 1000, 1, 1, 1, 1, 1, 1, 1)
  	</insert>
  </mapper>